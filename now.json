{
  "version": 2,
  "name": "apollo-zeit",
  "regions": ["bru"],
  "builds": [
    { "src": "www/package.json", "use": "@now/next" },
    { "src": "api/index.js", "use": "@now/node@canary" }
  ],
  "routes": [
    { "src": "/api", "dest": "api/index.js" },
    { "src": "/(.*)", "dest": "www/$1" }
  ]
}
